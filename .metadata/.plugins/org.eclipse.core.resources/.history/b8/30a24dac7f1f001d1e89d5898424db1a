package test0819;

public class Scores {
	// 필드
	private int korean;
	private int math;
	private int english;
	
	// 생성자
	public Scores() {
	}
	
	public Scores(int korean, int math, int english) {
		this.korean = korean;
		this.math = math;
		this.english = english;
	}
	
	// 메소드
	public int avrg() {
		int avrg =
		(this.korean + this.math + this.english)/3; 
		return avrg;
	}

}
