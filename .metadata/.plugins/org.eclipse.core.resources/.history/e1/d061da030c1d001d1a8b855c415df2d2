package pratice04;

public class BoardGame extends Game{
	// 필드
	private int peopleNum;
	
	// 생성자
	public BoardGame() {
		peopleNum = 0;
	}
	
	public BoardGame(String gameTitle, int peopleNum) {
		gameTitle = "";
		peopleNum = 0;
	}
	
	// 메소드
	@Override
	public void play() {
		super.play();
		System.out.println(this.peopleNum + "명이서" + this.gameTitle + "게임을 시작합니다");
	}

}
