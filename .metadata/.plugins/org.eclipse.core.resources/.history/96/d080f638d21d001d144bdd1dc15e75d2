package chapter08_abstract;

// 2022.08.17.(수)
// abstract을 통해서 추상클래스 선언
public abstract class shape {
	// 필드값 사용 가능
	private int x, y;
	
	// 일반 메소드 선언
	public void move(int x, int y) {
		this.x = x;
		this.y = y;
	}
	
	// 추상 메소드 선언
	// abstract 키워드를 사용해서 몸체(중괄호) 없이 이름만 작성
	public abstract void draw();
}
