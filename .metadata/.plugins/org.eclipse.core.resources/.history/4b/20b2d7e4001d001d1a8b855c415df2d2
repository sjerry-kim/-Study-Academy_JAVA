package pratice04;

public class Book {
	private String title;
	private String author;
	private int page;
	private boolean checkOut;
	
	public static int bookCount = 0;

	public static int getBookCount() {
		return bookCount;
	}
	
	public Book() {
		title = "미정";
		author = "미상";
		page = 0;
		checkOut = false;
	}
	
	public Book(String title, String author, int page) {
		checkOut = false;
	}
	
	
}
